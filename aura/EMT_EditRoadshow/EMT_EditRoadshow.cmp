                                    <aura:renderIf isTrue="{!v.showEventSize}"><h1><span class="slds-page-header__title slds-truncate" title="CREATE A NEW EVENT 3/6">{!$Label.c.create_new_event} 3/6</span></h1></aura:renderIf>
